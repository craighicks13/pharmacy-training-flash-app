package imobile.sessions.view{	import flash.display.MovieClip;	import flash.events.Event;		public class VerifyName extends MovieClip	{				public function VerifyName() 		{			addEventListener(Event.ADDED, clipAdded, false, 0, true);		}				private function clipAdded(e:Event):void		{			removeEventListener(Event.ADDED, clipAdded);			verifyWindow.content['VERIFY_PLACE'].addEventListener("click", proceed, false, 0, true);		}				private function proceed(e:Event):void		{			verifyWindow.content['VERIFY_PLACE'].removeEventListener("click", proceed);			nameMask.visible = false;			dispatchEvent(new Event("name_selected"));		}	}	}