package imobile.sessions.view {	import flash.display.MovieClip;	import imobile.sessions.controller.BubbleAnimation;		public class BubbleLeft extends MovieClip	{		private static var DIRECTION:String = "left";				public function BubbleLeft() 		{			alpha = 0;			BubbleAnimation.getInstance().setAnimation(this, DIRECTION);			BubbleAnimation.getInstance().animate();		}	}}