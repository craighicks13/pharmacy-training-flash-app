package imobile.sessions.view {	import flash.display.MovieClip;	import imobile.sessions.controller.BubbleAnimation;		public class BubbleRight extends MovieClip	{		private static var DIRECTION:String = "right";				public function BubbleRight() 		{			this.alpha = 0;			BubbleAnimation.getInstance().setAnimation(this, DIRECTION);			BubbleAnimation.getInstance().animate();		}	}}