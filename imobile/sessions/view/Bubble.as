package imobile.sessions.view {	import flash.display.MovieClip;	import imobile.sessions.controller.BubbleAnimation;		public class Bubble extends MovieClip	{		private static var DIRECTION:String = "none";				public function Bubble() 		{			this.alpha = 0;			BubbleAnimation.getInstance().setAnimation(this, DIRECTION);			BubbleAnimation.getInstance().animate();		}	}}