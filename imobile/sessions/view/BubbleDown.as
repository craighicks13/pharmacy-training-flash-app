package imobile.sessions.view {	import flash.display.MovieClip;	import imobile.sessions.controller.BubbleAnimation;		public class BubbleDown extends MovieClip	{		private static var DIRECTION:String = "down";				public function BubbleDown() 		{			this.alpha = 0;			BubbleAnimation.getInstance().setAnimation(this, DIRECTION);			BubbleAnimation.getInstance().animate();		}	}}